    "p-limit": "^7.2.0",
    "pinia": "^2.3.1",
    "vue": "^2.7.16",
    "vue-loader": "^17.4.2",
    "vue-material-design-icons": "^5.3.1",
    "vue-router": "^3.6.5",
    "vuedraggable": "^2.24.3",
    "webdav": "^5.8.0"
